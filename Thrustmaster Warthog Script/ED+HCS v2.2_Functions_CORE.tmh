	//FUNCTION: Init Fire
	int initFire(){
		if(Joystick[TG1] | Joystick[S2]){
			if(SilentRuningOFFonFire & sSilentRuning){	//ShutDown Silent Runing if it running
				if(!vSLonFire){
					vSLonFire=!vSLonFire;
					vSilentRuningBackup = sSilentRuning;	//Backup Actual Silent Runing State
					initToggleSilentRuning();
					if(DebugMode) printf("Shut Down Silent Runing\x0a");
				}
			}
			if(HardPointsonFireGameMode & !sHardPoints){initToggleHardpoints();}	//Deploy Hardpoint if HardPointsonFireGameMode is Enable
			if(OptimalPIPMode & !sExplorerMode){	//Backup Actual PIP Mode
//				if(vOptimalPIPFireBackup != vOptimalPIP & vOptimalPIP != 6){	//if the PIP it already 4 WEP 2 SYS (6) we don't save it again
//					vOptimalPIPFireBackup = vOptimalPIP;
//					if(DebugMode) printf("Backup PIP\x0a");
//				}
				initPIPBackup(OptimalPIPFireBackup);	//Backup Actual PIP Mode OptimalPIPFireBackup (1)
				initPIPPower(WEP,SYS);	//4 WEP 2 SYS (6)
			}
		}
		
		//Fire !!
		initPrimaryFire();
		initSecondaryFire();
		
		if(!Joystick[TG1] & !Joystick[S2]){
			if(OptimalPIPMode & !sExplorerMode){	//Restore Previous PIP Mode	
				initRestorePIPPower(vOptimalPIPFireBackup);	//Restore Previous PIP Mode
				if(DebugMode) printf("Restore PIP\x0a");
			}
			if(HardPointsonFireGameMode & StopFireRetractHardpoints & sHardPoints & !sExplorerMode){initToggleHardpoints();}//Retract Hardpoint if StopFireRetractHardpoints & HardPointsonFireGameMode are Enable
			if(vSilentRuningBackup!=0){	//Restore Silent Runing if it was running before fire
				vSilentRuningBackup = 0;
				initToggleSilentRuning();	//Restore Silent Runing State
				if(DebugMode) printf("Restore Silent Runing\x0a");
			}
	
			if(SilentRuningOFFonFire & sSilentRuning & vSLonFire){
				vSLonFire=!vSLonFire;
			}
			
		}
		
		//if(DebugMode)printf("vSLonFire [%d]\x0a", vSLonFire);
	}

	//FUNCTION: init PrimaryFire
	int initPrimaryFire(){
		if(Joystick[TG1] & !vPrimaryFire){
			ActKey(KEYON+DOWN+PrimaryFire);
			vPrimaryFire=!vPrimaryFire;
			//if(DebugMode) printf("TG1 Down\x0a");
		}
		else if(!Joystick[TG1] & vPrimaryFire){
			if(sExplorerMode & (ExploreModeFire == 0 | ExploreModeFire == 2)){
				DeferCall(6000, &ActKey, KEYON+UP+PrimaryFire);
				DeferCall(6500, &ActKey, KEYON+PULSE+EXEC("vPrimaryFire=!vPrimaryFire;"));
				//if(DebugMode) printf("TG1 Up ExplorerMode\x0a");
			}else{
				ActKey(KEYON+UP+PrimaryFire);
				vPrimaryFire=!vPrimaryFire;
				//if(DebugMode) printf("TG1 Up\x0a");
			}
		}
	}

	//FUNCTION: init SecondaryFire
	int initSecondaryFire(){
		if(Joystick[S2] & !vSecondaryFire){
			ActKey(KEYON+DOWN+SecondaryFire);
			vSecondaryFire=!vSecondaryFire;
			//if(DebugMode) printf("S2  Down\x0a");
		}
		else if(!Joystick[S2] & vSecondaryFire){
			if(sExplorerMode & (ExploreModeFire == 1 | ExploreModeFire == 2)){
				DeferCall(6000, &ActKey, KEYON+UP+SecondaryFire);
				DeferCall(6500, &ActKey, KEYON+PULSE+EXEC("vSecondaryFire=!vSecondaryFire;"));
				//if(DebugMode) printf("S2  Up ExplorerMode\x0a");
			}else{
				ActKey(KEYON+UP+SecondaryFire);
				vSecondaryFire=!vSecondaryFire;
				//if(DebugMode) printf("S2  Up\x0a");
			}
		}
	}
	
